<div class="contents row">
  <div class="container">
    <h3>投稿する</h3>
    <%= form_with(model: @content, local: true) do |form| %>
      <%= form.textfield :title, id:"content-title", placeholder: "title" %>
      <%= form.file_field :image, id:"content-image" %>
      <%= form.text_area :introduction, id:"content-introduction" %>
    <% end %>
  </div>
</div>